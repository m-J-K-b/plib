<div class="Navigation">
  {foreach item=nav name=nav key=n from=$NAV}
    {if $nav.active==1}
    {assign var="akt_nav" value=$nav.id}
    {assign var="nav_arr" value=$n}
    <span><a href="{$smarty_self}?id={$nav.id}_0_0" class="active">{$nav.name}</a></span>
    {else}
    <span><a href="{$smarty_self}?id={$nav.id}_0_0">{$nav.name}</a></span>
    {/if}
  {/foreach}
  <a href="suche.php" class="hl index">Index Search</a>
</div>
</div>


<div class="headbg">
</div>

<div class="clear"></div>

<div class="container2">
  {if $SUBNAV.0.id >0}
	{foreach item=subnav name=subnav key=sn from=$SUBNAV}
		{if $subnav.active==1}
			{assign var="akt_subnav" value=$subnav.id}
			{assign var="subnav_arr" value=$sn}
			{assign var="akt_topic" value=$subnav.name}
		{/if}
	{/foreach}
  {/if}


	{if $TOPIC.0.id!=''}
		{foreach item=topic name=topic from=$TOPIC}
			{if $topic.active==1}
			  {assign var="akt_topic" value=$topic.name}
			  {assign var="akt_topictext" value=$topic.text}
			  {assign var="akt_topicid" value=$topic.id}
			  {else}
			{/if}
		{/foreach}
	{/if}


	{if $NAV.$nav_arr.intro_text==''}
	{elseif isset($NAV.$nav_arr.intro_text) && !$SUBNAV.$subnav_arr.text && !$akt_topictext}
   	{elseif isset($SUBNAV.$subnav_arr.text) && !$akt_topictext}
    {elseif isset($akt_topictext)}
    {/if}
</div>

<div class="container2">

	{if $NAV.$nav_arr.intro_text==''}
	<div class="introtext2">.&nbsp;.&nbsp;.&nbsp;</div>
	<div class="introtext2abschluss"></div>
	{elseif isset($NAV.$nav_arr.intro_text) && !$SUBNAV.$subnav_arr.text && !$akt_topictext}
	<div class="sprachindikator"></div>
	<div class="introtext2">{$NAV.$nav_arr.intro_text|linker}</div>
	<div class="introtext2abschluss"></div>

   	{elseif isset($SUBNAV.$subnav_arr.text) && !$akt_topictext}
    <div class="introtext2">{$SUBNAV.$subnav_arr.text|linker}</div>
	<div class="ordnerindikator"></div>

    {elseif isset($akt_topictext)}
    <div class="introtext2">
    {$akt_topictext|linker}</div>
    <div class="themenindikator"></div>
    {/if}



	<div class="spalte">

  {if $SUBNAV.0.id >0}
  <div class="spaltenheadline"><img src="_templates/_img/ordner_icon.gif" alt="" /></div>
  	<ol>
	{foreach item=subnav name=subnav key=sn from=$SUBNAV}
		{if $subnav.active==1}
			{assign var="akt_subnav" value=$subnav.id}
			{assign var="subnav_arr" value=$sn}
			{assign var="akt_topic" value=$subnav.name}
			<li><a href="{$smarty_self}?id={$akt_nav}_{$subnav.id}_0" class="standardlink"><!-- {$sn} --> {$subnav.name}</a></li>
			{else}
			<li><a href="{$smarty_self}?id={$akt_nav}_{$subnav.id}_0" class="grey"><!-- {$sn} --> {$subnav.name}</a></li>
		{/if}
	{/foreach}
	</ol>
  {/if}

  </div>

	{if $TOPIC.0.id==''}
	<div class="spalte extras invisible">&nbsp;</div>
	{else}
	<div class="spalte extras">
	<div class="spaltenheadline"><img src="_templates/_img/themen_icon.gif" alt="" /></div>
	  {if $TOPIC.0.id!=''}
		<ol>
		  {foreach item=topic name=topic from=$TOPIC}
				{if $topic.active==1}
				  {assign var="akt_topic" value=$topic.name}
				  {assign var="akt_topictext" value=$topic.text}
				  {assign var="akt_topicid" value=$topic.id}
				  <li><a href="{$smarty_self}?id={$akt_nav}_{$akt_subnav}_{$topic.id}" class="standardlink">{$topic.name}</a></li>
				  {else}
				  <li><a href="{$smarty_self}?id={$akt_nav}_{$akt_subnav}_{$topic.id}" class="grey">{$topic.name}</a></li>
				{/if}
			{/foreach}
		</ol>
		{/if}
	  </div>
	{/if}

      <div class="spaltedia">
    {if isset($CONTENT.0.file) && $akt_subnav>0}
   <div class="spaltenheadline ieplus"><img src="_templates/_img/dateien_icon.gif" alt="" /></div>
		<p class="titel3">PDFs</p>
		  {foreach key=f item=text from=$CONTENT}
			<a name="{$text.headline}"></a>
			<div><a name="{$f}"></a><h2 class="titel">{$text.headline}</h2>
			<p class="pdfdescription">{$text.content|linker}</p>

			{if $text.image_position=='short' && $text.image!=''}
			<div class="customimage">
			<img src="_uploads/_img/{$text.image}" alt="{$text.headline}" class="artimg" />
			</div>
			{/if}
			{if $text.extratext!='' || ($text.image_position=='long' && $text.image!='')}<a name="{$f}"></a><div id="short{$f}" class="show"><a href="#{$f}" onclick="SwapLayer('long{$f}','short{$f}')">read more...</a></div>
			<div id="long{$f}" class="hide">
				{if $text.image_position=='long' && $text.image!=''}
					<div class="customimage">
					<img src="_uploads/_img/{$text.image}" alt="{$text.headline}" class="artimg" />
					</div>
				{/if}
			<div class="longtext">
				<h1 class="longtextheadline">{$text.extrahead}</h1>
				{$text.extratext}
			</div>
			<a href="#{$f}" onclick="SwapLayer('short{$f}','long{$f}')">close</a>
			</div>{/if}
			{if $text.file!=''}
				<!-- ({$text.date}) -->
				<ul class="download">
			 		<li><a href="_uploads/_pdfs/{$text.file}"><img src="_templates/_img/pdf.gif" alt="" /> download: {$text.file} </a></li>
				</ul>
			{else}
			 	<br />
			{/if}
			 <ul class="tagul">
			 {if $text.tag!=''}
			 <li>Stichworte:</li>
			 	{foreach name=tag key=t item=tags from=$text.tag}
			 		 {if $smarty.foreach.tag.last==TRUE}
			 		 <li><a href="suche.php?tag={$tags.id}&amp;name={$tags.tagname}">{$tags.tagname}</a></li>
			 		 {else}
			 		 <li><a href="suche.php?tag={$tags.id}&amp;name={$tags.tagname}">{$tags.tagname}</a>,</li>
			 		 {/if}
			 	{/foreach}
			 	{else}
			 	<li>&nbsp;</li>
			 	{/if}
			 </ul>
		  </div>
		  {/foreach}
	{elseif isset($TOPIC.direkt.headline)}
	<div class="spaltenheadline ieplus"><img src="_templates/_img/dateien_icon.gif" alt="" /></div>
	<div><div class="titel3">PDFs</div></div>
	{foreach item=direkt from=$TOPIC}
		<a name="{$text.headline}"></a>
	 <div><a name="{$f}"></a><h2 class="titel">{$direkt.headline}</h2>
			<p class="pdfdescription">{$direkt.content}</p>
			{if $direkt.image_position=='short' && $direkt.image!=''}
			<div class="customimage">
			<img src="_uploads/_img/{$direkt.image}" alt="{$direkt.headline}" class="artimg" />
			</div>
			{/if}
			{if $direkt.extratext!='' || ($text.image_position=='long' && $text.image!='')}<div id="short{$f}" class="show"><a href="#{$f}" onclick="SwapLayer('long{$f}','short{$f}')">read more...</a></div>
			<div id="long{$f}" class="hide">
				{if $direkt.image_position=='long' && $direkt.image!=''}
					<div class="customimage">
					<img src="_uploads/_img/{$direkt.image}" alt="{$direkt.headline}" class="artimg" />
					</div>
				{/if}
			<div class="longtext">
				<h1 class="longtextheadline">{$direkt.extrahead}</h1>
				{$direkt.extratext}
			</div>
			<a href="#{$f}" onclick="SwapLayer('short{$f}','long{$f}')">close</a>
			</div>{/if}
			{if $direkt.file!=''}
				<!-- ({$direkt.date}) -->
				<ul class="download">
			 		<li><a href="_uploads/_pdfs/{$direkt.file}"><img src="_templates/_img/pdf.gif" alt="" /> download: {$direkt.file} </a></li>
				</ul>
			{else}
			 	<br />
			{/if}
			 <ul class="tagul">
			 {if $text.tag!=''}
			 <li>Stichworte:</li>
			 	{foreach name=tag key=t item=tags from=$direkt.tag}
			 		 {if $smarty.foreach.tag.last==TRUE}
			 		 <li><a href="suche.php?tag={$tags.id}&amp;name={$tags.tagname}">{$tags.tagname}</a></li>
			 		 {else}
			 		 <li><a href="suche.php?tag={$tags.id}&amp;name={$tags.tagname}">{$tags.tagname}</a>,</li>
			 		 {/if}
			 	{/foreach}
			 	{else}
			 	<li>&nbsp;</li>
			 	{/if}
			 </ul>
		  </div>
	{/foreach}
    {elseif isset($CONTENT.0.error) && $akt_topicid>0}
      <div class="spaltenheadline"><img src="_templates/_img/dateien_icon.gif" alt="" /></div>
      	  {if isset($akt_topic)}<p><div class="titel3">{$akt_topic}</div></p>{/if}
          <h2 class="titel">{$CONTENT.0.error}
          <div>{$CONTENT.0.errortext}<br/></div></h2>
    {/if}
	</div>
